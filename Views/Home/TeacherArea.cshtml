@model IEnumerable<FirstTrade_.Models.ViewModels.TeacherAreaVM>

@{
    ViewBag.Title = "TeacherArea";
}

<h2>TeacherArea</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Member)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Member)
            </td>
            <td>
                @Html.ActionLink("Details", "MemberDetails", new { userid = item.Member })
            </td>
        </tr>
    }

</table>
